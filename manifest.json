{
  "manifest": {
    "name": "Custom Community Visualization",
    "version": "1.0.0",
    "description": "A custom community visualization for Looker Studio",
    "author": {
      "name": "Your Name",
      "email": "your.email@example.com"
    },
    "api_version": "1.0"
  },
  "data": [
    {
      "id": "myData",
      "label": "My Data",
      "elements": [
        {
          "id": "dimension",
          "type": "DIMENSION",
          "label": "Dimension",
          "options": {
            "min": 1,
            "max": 10
          }
        },
        {
          "id": "metric",
          "type": "METRIC",
          "label": "Metric",
          "options": {
            "min": 1,
            "max": 10
          }
        }
      ]
    }
  ],
  "visualization": {
    "js_library": "main.js"
  },
  "style": {
    "js_library": "styles.js"
  }
}

